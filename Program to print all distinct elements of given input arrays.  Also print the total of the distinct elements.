/*
Program to print all distinct elements of given input arrays. 
Also print the total of the distinct elements.
*/

#include<iostream>
using namespace std;

int main()
{
	int arr1[10];
	int arr2[20];
	int n;
	
	cout<<"\nHow many number of elements in the array 1 :- ";
	cin>>n;
	
	cout<<"\nEnter the number of elements of array 1 :- ";
	for(int i=0;i<n;i++)
	{
		cin>>arr1[i];
	}
	
	cout<<"\nEnter the number of elements of array 2 :- ";
	for(int i=0;i<n;i++)
	{
		cin>>arr2[i];
	}
	
	int arr3[10];
	for(int z=0;z<n;z++)
	{
		arr3[z]=0;
	}
	int k=0;
	
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
		{
			if(arr1[i] == arr2[j])
			{
				arr3[k] = arr1[i];
				k++;
			}
		}
	}
	
	cout<<"\nCommon elements are as follows :- ";
	for(int i=0;i<n;i++)
	{
		cout<<arr3[i]<<endl;
	}
	
	return 0;
}

/*
Output :-

How many number of elements in the array 1 :- 5

Enter the number of elements of array 1 :- 
4
5
6
7
8

Enter the number of elements of array 2 :- 
6
7
8
9
10

Common elements are as follows :- 
6
7
8
0
0

--------------------------------
*/
