/* 
Print the Armstrong numbers between the two intervals.
*/

#include<iostream>
using namespace std;

int main()
{
	int start,end;
	int num;
	int rem;
	int required_num = 0;
	int Number;
	int count=0;
	
	cout<<"\nEnter the start value of range :- ";
	cin>>start;
	
	cout<<"\nEnter the end value of range :- ";
	cin>>end;
	
	for(int i=start;i<=end;i++)
	{
		Number = i;
		num = i;
		int temp = num;
		
		while(temp!=0)
		{	
			temp=temp/10;
			count++;	
		}
		
		int temp2=1;
		
		while(num!=0)
		{
			rem = num % 10;
			for(int i=1;i<=count;i++)
			{
				temp2 = temp2 * rem;
			}
			num = num / 10;
			required_num = required_num + temp2;
			temp2=1;
		}
		
		if(Number == required_num)
		{
			cout<<"\n\n"<<Number<<" Armstrong Number :- "<<required_num<<"\n";
		}
		else
		{
			cout<<"\n"<<Number<<" Given number is not a Armstrong Number...";
		}
		
		required_num = 0;
		count=0;

	}	
	return 0;
}

